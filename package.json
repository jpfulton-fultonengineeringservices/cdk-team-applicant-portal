{
  "name": "cdk-team-applicant-portal",
  "version": "1.2.0",
  "description": "CDK project for a Cognito-authenticated applicant portal served via CloudFront",
  "main": "bin/app.js",
  "license": "Apache-2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jpfulton-fultonengineeringservices/cdk-team-applicant-portal.git"
  },
  "homepage": "https://github.com/jpfulton-fultonengineeringservices/cdk-team-applicant-portal#readme",
  "bugs": {
    "url": "https://github.com/jpfulton-fultonengineeringservices/cdk-team-applicant-portal/issues"
  },
  "packageManager": "yarn@4.7.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "clean": "find . -name '*.d.ts' -not -path './node_modules/*' -delete && find . -name '*.js' -not -path './node_modules/*' -not -path './cdk.out/*' -delete",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.993.0",
    "aws-cdk-lib": "2.238.0",
    "cdk-nag": "^2.28.0",
    "constructs": "^10.5.1",
    "cookie": "^1.1.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "25.3.0",
    "aws-cdk": "2.1106.1",
    "esbuild": "^0.27.3",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
