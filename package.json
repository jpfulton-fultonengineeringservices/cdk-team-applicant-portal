{
  "name": "cdk-team-applicant-portal",
  "version": "0.1.0",
  "description": "CDK project for a Cognito-authenticated applicant portal served via CloudFront",
  "main": "bin/app.js",
  "private": true,
  "packageManager": "yarn@4.7.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "clean": "find . -name '*.d.ts' -not -path './node_modules/*' -delete && find . -name '*.js' -not -path './node_modules/*' -not -path './cdk.out/*' -delete"
  },
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.993.0",
    "aws-cdk-lib": "2.221.0",
    "cdk-nag": "^2.28.0",
    "constructs": "^10.0.0",
    "cookie": "^0.6.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/cookie": "^0.6.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "22.7.9",
    "aws-cdk": "2.1100.1",
    "esbuild": "^0.24.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  }
}
